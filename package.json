{
  "name": "indieauth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.cjs",
    "client": "webpack-dev-server --open --mode development --config webpack.config.cjs",
    "server": "nodemon ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run build && node ./bin/www"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-react-views": "^0.11.0",
    "morgan": "~1.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
