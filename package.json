{
  "name": "indieauth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.cjs",
    "dev": "nodemon -e js,jsx ./bin/www",
    "start": "NODE_ENV=production node ./bin/www"
  },
  "babel": {
    "presets":
      ["@babel/preset-env",
         ["@babel/preset-react",
            {"runtime": "classic"}
         ]
      ]
  },
  "nodemonConfig": {
    "ignore": ["dist/"]
  },
  "eslintConfig": {
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["dist/"]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.1",
    "morgan": "~1.9.1",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "session-file-store": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
